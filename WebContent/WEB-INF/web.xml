<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" id="WebApp_ID" version="3.1">
  <display-name>TheFourHorsemen</display-name>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <description>MySQL User</description>
    <param-name>mysqlUser</param-name>
    <param-value>root</param-value>
  </context-param>
  <context-param>
    <description>MySQL Password</description>
    <param-name>mysqlPassword</param-name>
    <param-value>toor</param-value>
  </context-param>
  <context-param>
    <description>JDBC URL</description>
    <param-name>jdbcURL</param-name>
    <param-value>jdbc:mysql://localhost/fyp</param-value>
  </context-param>
</web-app>